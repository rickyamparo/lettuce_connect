<%= link_to "Register Account", new_user_path %>
<% if current_user %>
  <%= link_to "Logout", logout_path %>
  <h2>Hello <%= "#{current_user.first_name} #{current_user.last_name}"%> </h2>
  <h3>Signed in under: <%= "#{current_user.email}" %>  </h3>
<% else %>
  <%= link_to "Login", login_path %>
<% end %>
